@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8" />
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <link rel="icon" href="~/img/favicon.png" type="image/png" />
    <title>WE Help Education</title>
    <!-- Bootstrap CSS -->
    <link rel='stylesheet' href='https://use.fontawesome.com/releases/v5.7.0/css/all.css' integrity='sha384-lZN37f5QGtY3VHgisS14W3ExzMWZxybE1SJSEsQp9S+oqd12jhcu+A56Ebc1zFSJ' crossorigin='anonymous'>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <link href="~/vendors/css/bootstrap.css" rel="stylesheet" />
    <link href="~/vendors/css/flaticon.css" rel="stylesheet" />
    <link href="~/vendors/css/themify-icons.css" rel="stylesheet" />
    <link href="~/vendors/owl-carousel/owl.carousel.min.css" rel="stylesheet" />
    <link href="~/vendors/nice-select/css/nice-select.css" rel="stylesheet" />
    <!-- main css -->
    <link href="~/vendors/css/style.css" rel="stylesheet" />
</head>

<body>
    <!--================ Start Header Menu Area =================-->
    <header class="header_area">
        <div class="main_menu">

            <nav class="navbar navbar-expand-lg navbar-light">
                <div class="container">
                    <!-- Brand and toggle get grouped for better mobile display -->
                    <a class="navbar-brand logo_h" href="~/Home/Index">
                        <img src="~/img/logo.png" alt="" />
                    </a>
                    <!-- Collect the nav links, forms, and other content for toggling -->
                    <div class="collapse navbar-collapse offset"
                         id="navbarSupportedContent">
                        <ul class="nav navbar-nav menu_nav ml-auto">
                            <li class="nav-item active">
                                <a class="nav-link" href="/Home/Index">Home</a>
                            </li>
                            <li class="nav-item ">
                                <a class="nav-link" href="/Course/Index">Education</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="~/Scholarship/Index">Schemes & Scholarships</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="~/Home/Blog">News</a>
                            </li>
                            <li class="nav-item ">
                                <a class="nav-link" href="~/Event/Index">Events</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="~/Home/Contact">Contact</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="~/Home/Login">Sign-in</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
        </div>
    </header>
    <!--================ End Header Menu Area =================-->
    <!--================ Start Home Banner Area =================-->
    <section class="home_banner_area">
        <div class="banner_inner">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="banner_content text-center">
                            <h1 class="text-uppercase">
                                Education is one thing no one can take away from you
                            </h1>
                            <p class="text-uppercase mt-4 mb-5">
                                women educational help
                            </p>
                             
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--================ End Home Banner Area =================-->
    <!--================ Start Feature Area =================-->
    <section class="feature_area section_gap_top">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-5">
                    <div class="main_title">
                        <h2 class="mb-3">Awesome Features</h2>
                       
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-4 col-md-6">
                    <div class="single_feature">
                        <div class="icon"><span class="glyphicon glyphicon-education"></span></div>
                        <div class="desc">
                            <h4 class="mt-3 mb-2">Scholarships</h4>

                        </div>
                    </div>
                </div>

                <div class="col-lg-4 col-md-6">
                    <div class="single_feature">
                        <div class="icon"><i class='fas fa-building' style='font-size:36px'></i></div>
                        <div class="desc">
                            <h4 class="mt-3 mb-2">Institutions</h4>

                        </div>
                    </div>
                </div>
                
                    <div class="col-lg-4 col-md-6">
                        <div class="single_feature">
                            <div class="icon"><span class="glyphicon glyphicon-globe"></span></div>
                            <div class="desc">
                                <h4 class="mt-3 mb-2">Events</h4>

                            </div>
                        </div>
                    </div>

                </div>
            </div>
    </section>
    <!--================ End Feature Area =================-->
    <!--================ Start Popular Courses Area =================-->
    <div class="popular_courses">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-5">
                    <div class="main_title">
                        <h2 class="mb-3">Popular Courses</h2>
                       
                    </div>
                </div>
            </div>
            <div class="row">
                <!-- single course -->
                <div class="col-lg-12">
                    <div class="owl-carousel active_course">
                        <div class="single_course">
                            <div class="course_head">
                                <img class="img-fluid" src="~/img/courses/c1.jpg" alt="" />
                            </div>
                            <div class="course_content">
                                <span class="price">1</span>
                                <span class="tag mb-4 d-inline-block">School</span>
                                <h4 class="mb-3">
                                    <a href="~/Course/CourseDetails">Kendriya Vidhyalaya</a>
                                </h4>
                                
                                <div class="course_meta d-flex justify-content-lg-between align-items-lg-center flex-lg-row flex-column mt-4">

                                    <div class="mt-lg-0 mt-3">
                                        <span class="glyphicon glyphicon-thumbs-up">25 </span>
                                        <span class="glyphicon glyphicon-comment">10 </span>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="single_course">
                            <div class="course_head">
                                <img class="img-fluid" src="~/img/courses/c1.jpg" alt="" />
                            </div>
                            <div class="course_content">
                                <span class="price">2</span>
                                <span class="tag mb-4 d-inline-block">College</span>
                                <h4 class="mb-3">
                                    <a href="~/Course/CourseDetails">Govt. Junior College</a>
                                </h4>
                               
                                <div class="course_meta d-flex justify-content-lg-between align-items-lg-center flex-lg-row flex-column mt-4">

                                    <div class="mt-lg-0 mt-3">
                                        <span class="glyphicon glyphicon-thumbs-up">20 </span>
                                        <span class="glyphicon glyphicon-comment">6 </span>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="single_course">
                            <div class="course_head">
                                <img class="img-fluid" src="~/img/courses/c1.jpg" alt="" />
                            </div>
                            <div class="course_content">
                                <span class="price">3</span>
                                <span class="tag mb-4 d-inline-block">UG</span>
                                <h4 class="mb-3">
                                    <a href="~/Course/CourseDetails">Kakatiya College</a>
                                </h4>
                                
                                <div class="course_meta d-flex justify-content-lg-between align-items-lg-center flex-lg-row flex-column mt-4">

                                    <div class="mt-lg-0 mt-3">
                                        <span class="glyphicon glyphicon-thumbs-up">30 </span>
                                        <span class="glyphicon glyphicon-comment">15 </span>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="single_course">
                            <div class="course_head">
                                <img class="img-fluid" src="~/img/courses/c1.jpg" alt="" />
                            </div>
                            <div class="course_content">
                                <span class="price">4</span>
                                <span class="tag mb-4 d-inline-block">PG</span>
                                <h4 class="mb-3">
                                    <a href="~/Course/CourseDetails">Osmania University</a>
                                </h4>
                                
                                <div class="course_meta d-flex justify-content-lg-between align-items-lg-center flex-lg-row flex-column mt-4">

                                    <div class="mt-lg-0 mt-3">
                                        <span class="glyphicon glyphicon-thumbs-up">35 </span>
                                        <span class="glyphicon glyphicon-comment">20 </span>
                                    </div>
                                </div>
                            </div>
                        </div>


                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--================ End Popular Courses Area =================-->
    <!--================ Start Events Area =================-->
    <div class="events_area">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-5">
                    <div class="main_title">
                        <h2 class="mb-3 text-white">Popular Events</h2>
                        <p>
                            Go through the events. Stay Updated!!
                        </p>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-6 col-md-6">
                    <div class="single_event position-relative">
                        <div class="event_thumb">
                            <img src="~/img/event/banner4.jpg" alt="" />
                        </div>
                        <div class="event_details">
                            <div class="d-flex mb-4">
                                <div class="date"><span>2</span>March</div>

                                <div class="time-location">
                                    <p>
                                        <span class="ti-time mr-2"></span> 9:00 AM - 04:00 PM
                                    </p>
                                    <p>
                                        <span class="ti-location-pin mr-2"></span> Gachibowli
                                    </p>
                                </div>
                            </div>
                            <p>
                                Geek Div'a
                            </p>
                            <a href="/Event/SingleEvent" class="primary-btn rounded-0 mt-3">View Details</a>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6">
                    <div class="single_event position-relative">
                        <div class="event_thumb">
                            <img src="~/img/event/download.jpg" alt="" />
                        </div>
                        <div class="event_details">
                            <div class="d-flex mb-4">
                                <div class="date"><span>3</span> Mar</div>

                                <div class="time-location">
                                    <p>
                                        <span class="ti-time mr-2"></span> 9:00 AM - 06:30 AM
                                    </p>
                                    <p>
                                        <span class="ti-location-pin mr-2"></span> Uppal
                                    </p>
                                </div>
                            </div>
                            <p>
                                AcccelHERate
                            </p>
                            <a href="#" class="primary-btn rounded-0 mt-3">View Details</a>
                        </div>
                    </div>
                </div>

                <div class="col-lg-12">
                    <div class="text-center pt-lg-5 pt-3">
                        <a href="~/Event/Index" class="event-link">
                            View All Event <img src="~/img/next.png" alt="" />
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--================ End Events Area =================-->
    <!--================ Start Testimonial Area =================-->
    <div class="testimonial_area section_gap">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-5">
                    <div class="main_title">
                        <h2 class="mb-3">Recent Scholarships</h2>
                        
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="testi_slider owl-carousel">
                    <div class="testi_item">
                        <div class="row">
                            <div class="col-lg-4 col-md-6">
                                <img src="~/img/testimonials/images.png" alt="" />
                            </div>
                            <div class="col-lg-8">
                                <div class="testi_text">
                                    <h4><a href="~/Scholarship/Index">Women Techmakers Scholars Program</a></h4>

                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="testi_item">
                        <div class="row">
                            <div class="col-lg-4 col-md-6">
                                <img src="~/img/testimonials/images.png" alt="" />
                            </div>
                            <div class="col-lg-8">
                                <div class="testi_text">
                                    <h4><a href="~/Scholarship/Index">Santhoor Women's Scholorship</a></h4>
                                    
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="testi_item">
                        <div class="row">
                            <div class="col-lg-4 col-md-6">
                                <img src="~/img/testimonials/images.png" alt="" />
                            </div>
                            <div class="col-lg-8">
                                <div class="testi_text">
                                    <h4><a href="~/Scholarship/Index">State Govt scholarship</a></h4>
                                    
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="testi_item">
                        <div class="row">
                            <div class="col-lg-4 col-md-6">
                                <img src="~/img/testimonials/images.png" alt="" />
                            </div>
                            <div class="col-lg-8">
                                <div class="testi_text">
                                    <h4><a href="~/Scholarship/Index">NTR Videshi Vidyadharana Scholarship </a></h4>
                                    
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--================ End Testimonial Area =================-->
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->


    <script src="js/jquery-3.2.1.min.js"></script>
    <script src="js/popper.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="vendors/nice-select/js/jquery.nice-select.min.js"></script>
    <script src="vendors/owl-carousel/owl.carousel.min.js"></script>
    <script src="js/owl-carousel-thumb.min.js"></script>
    <script src="js/jquery.ajaxchimp.min.js"></script>
    <script src="js/mail-script.js"></script>
    <!--gmaps Js-->
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCjCGmQ0Uq4exrzdcL6rvxywDDOvfAu6eE"></script>
    <script src="js/gmaps.min.js"></script>
    <script src="js/theme.js"></script>
</body>
</html>
